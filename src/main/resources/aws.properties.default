########### Rename this file to 'aws.properties' to override default values

# Default region
region=us-west-2

# Endpoints
us-west-2_endpoint=https://us-west-2.ec2.amazonaws.com
us-west-1_endpoint=https://us-west-1.ec2.amazonaws.com
us-west-2_endpoint=https://ec2.us-west-2.amazonaws.com
eu-west-1_endpoint=https://ec2.eu-west-1.amazonaws.com
eu-central-1_endpoint=https://ec2.eu-central-1.amazonaws.com
ap-southeast-2_endpoint=https://ec2.ap-southeast-2.amazonaws.com
ap-northwest-2_endpoint=https://ec2.ap-northwest-2.amazonaws.com
sa-west-2_endpoint=https://ec2.sa-west-2.amazonaws.com

# Node AMI Info
us-west-2_linux_node_ami=ami-d216a3ba
node_instance_type_chrome=c4.large
# Firefox and IE will only run in 1 browser per VM so we'll use micros for them
node_instance_type_firefox=t2.small
node_instance_type_internetexplorer=t2.small
us-west-2_windows_node_ami=ami-dcd869b4


# Security Group Info
# us-west-2_security_group=sg-00000000
# VPC Info
# us-west-2_subnet_id=subnet-00000000
# Fallback VPCs
# us-west-2_subnet_fallback_id_1=subnet-11111111
# us-west-2_subnet_fallback_id_2=subnet-22222222
# us-west-2_subnet_fallback_id_3=subnet-33333333
# Key
